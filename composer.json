{
    "name": "vovarpd/puphpeteer",
    "description": "A fork of nesk/puphpeteer with Laravel 9 support. Puppeteer bridge for PHP, supporting the entire API.",
    "keywords": [
        "php",
        "puppeteer",
        "headless-chrome",
        "testing",
        "web",
        "developer-tools",
        "automation"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Johann Pardanaud",
            "email": "pardanaud.j@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "composer/semver": "^3.3",
        "psr/log": "^1.0|^3.0",
        "vovarpd/rialto": "^2.0"
    },
    "require-dev": {
        "monolog/monolog": "^2.0|^3.0",
        "phpunit/phpunit": "^9.0|^10.0",
        "symfony/process": "^6.0",
        "symfony/console": "^6.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:vovarpd/rialto.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nesk\\Puphpeteer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nesk\\Puphpeteer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": "npm install",
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
